#include <iostream>
using namespace std;

int main()
{
    char A[]="Welcome To My World";
    int i;
    int word =1;
    
    for(i=0; A[i]!='\0';i++)
    {
        if(A[i]==' ' && A[i-1]!=' ')
        {
            word++;
        }
        
    }
    
    cout<<"The numbers of word are: "<<word;
   
    
    
}
